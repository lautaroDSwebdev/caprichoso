---
import Layout from "@/layouts/Layout.astro";
import data from "../mock/data.json";
const slethData = data.portfolio.portfolioPage.sleth;
---

<Layout title="Banco Santander">
  <div>
    <h2 class="text-white">Banco Santander</h2>
    <div class="flex flex-col gap-3 max-width iframe-style">
      {slethData.map((e) => <iframe src={e.url} allowfullscreen />)}
    </div>
  </div>
</Layout>

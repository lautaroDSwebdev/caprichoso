---
import Layout from "@/layouts/Layout.astro";
import data from "../mock/data.json";
const tacserData = data.portfolio.portfolioPage.tacser;
const {title} = Astro.props 
---

<Layout title="Tacser">
  <div> 
    <h2 class="text-white titles">Tacser</h2>
    <div class="flex flex-col gap-3 max-width iframe-style">
      {tacserData.map((e) => <iframe src={e.url} allowfullscreen />)}
    </div>
  </div>
</Layout>
